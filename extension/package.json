{
  "name": "snip",
  "displayName": "Snip",
  "publisher": "irere",
  "description": "Share code with your peers",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "snip-sidebar-view",
          "title": "Snip",
          "icon": "media/quote.svg"
        }
      ]
    },
    "views": {
      "snip-sidebar-view": [
        {
          "type": "webview",
          "id": "snip-sidebar",
          "name": "Snip",
          "icon": "media/quote.svg",
          "contextualTitle": "Snip"
        }
      ]
    },
    "commands": [
      {
        "command": "snip.authenticate",
        "title": "Authenticate",
        "category": "Snip"
      },
      {
        "command": "snip.reloadSidebar",
        "title": "Reload Sidebar Webview",
        "category": "Snip"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "pnpm build:rollup && webpack --mode production",
    "build:rollup": "rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript --environment INCLUDE_DEPS,BUILD:production",
    "watch": "concurrently  \"webpack --mode development --watch\" \"rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript -w\"",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@tsconfig/svelte": "^5.0.2",
    "@types/koa": "^2.14.0",
    "@types/koa__router": "^12.0.4",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "rollup": "^4.9.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^7.1.6",
    "svelte": "^4.2.8",
    "svelte-preprocess": "^5.1.3",
    "ts-loader": "^9.5.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@koa/router": "^12.0.1",
    "koa": "^2.15.0",
    "node-fetch": "^3.3.2"
  }
}
